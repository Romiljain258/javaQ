import java.util.Scanner;

public class Main
{
	public static void main(String[] args)
	{
		Scanner obj=new Scanner(System.in);
		 int T=obj.nextInt();
		
		while(T>0)
		{
		long N=obj.nextInt();
		int c=0;
			if(N%2==0)
			 {
			while(N!=1)
			{
				N=N/2;
				c++;
			}
			System.out.print(c);

			}
			else
			{
			System.out.print(0);
			}	
			T--;
			System.out.println(" ");
		}
	}
}
